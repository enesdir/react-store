		"prettier": "^3.4.2",
		"prettier-plugin-jsdoc": "^1.3.2",
		"prettier-plugin-pkg": "^0.18.1",
		"prettier-plugin-sh": "^0.14.0",
		"turbo": "^2.3.3",
		"typescript": "5.7.3",
		"vite": "^6.0.7",
		"vite-plugin-inspect": "^10.0.6",
		"vite-plugin-pwa": "^0.21.1"
	}
